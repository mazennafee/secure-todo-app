{
    "name": "secure-todo-backend",
    "version": "1.0.0",
    "description": "Secure ToDo App Backend API",
    "main": "src/server.js",
    "type": "module",
    "scripts": {
        "start": "node src/server.js",
        "dev": "node --watch src/server.js",
        "test": "NODE_ENV=test jest --forceExit",
        "test:watch": "NODE_ENV=test jest --watch",
        "test:coverage": "NODE_ENV=test jest --coverage --forceExit",
        "migrate": "PGPASSWORD=$DB_PASSWORD psql -h $DB_HOST -U $DB_USER -d $DB_NAME -f scripts/migrate.sql",
        "seed": "node scripts/seed.js"
    },
    "keywords": [
        "todo",
        "security",
        "jwt",
        "express"
    ],
    "author": "Mazennafee",
    "license": "MIT",
    "dependencies": {
        "express": "^4.18.2",
        "pg": "^8.11.3",
        "bcrypt": "^5.1.1",
        "jsonwebtoken": "^9.0.2",
        "helmet": "^7.1.0",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express-validator": "^7.0.1",
        "express-rate-limit": "^7.1.5",
        "hpp": "^0.2.3",
        "xss-clean": "^0.1.4",
        "cookie-parser": "^1.4.6",
        "multer": "^1.4.5-lts.1"
    },
    "devDependencies": {
        "nodemon": "^3.0.2",
        "jest": "^29.7.0",
        "supertest": "^6.3.3"
    }
}